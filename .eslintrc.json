{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:vue/essential",
    "airbnb-base",
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          [
            "@",
            "./src/"
          ],
          [
            "@scss",
            "./src/assets/"
          ]
        ],
        "extensions": [
          ".js",
          ".vue"
        ]
      }
    },
    "import/extensions": [
      ".js",
      ".vue"
    ]
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {}
}